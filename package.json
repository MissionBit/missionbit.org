{
  "name": "missionbit-website",
  "version": "1.0.0",
  "description": "Mission Bit's website",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next",
    "build": "next build",
    "postbuild": "next-on-netlify && ./bin/generate-sitemap.js",
    "export": "npm run build && next export",
    "start": "next start",
    "fix": "eslint --fix --ext js,jsx,ts,tsx .",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "posttest": "eslint --ext js,jsx,ts,tsx .",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/missionbit/missionbit-website.git"
  },
  "keywords": [],
  "author": "Mission Bit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/missionbit/missionbit-website/issues"
  },
  "homepage": "https://github.com/missionbit/missionbit-website#readme",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@sendgrid/mail": "^7.2.5",
    "@sentry/browser": "^5.22.3",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.11.0",
    "@types/google.analytics": "0.0.40",
    "@types/gtag.js": "0.0.3",
    "@types/micro": "^7.3.3",
    "@types/micro-cors": "^0.1.1",
    "clsx": "^1.1.1",
    "cross-fetch": "^3.0.6",
    "dayjs": "^1.8.35",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "material-ui-popup-state": "^1.7.1",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "mjml": "^4.6.3",
    "mjml-react": "^1.0.57",
    "next": "^9.5.5",
    "next-compose-plugins": "^2.2.0",
    "next-on-netlify": "^2.5.1",
    "next-optimized-images": "^2.6.2",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "responsive-loader": "^2.1.1",
    "sharp": "^0.26.0",
    "sitemap": "^6.3.0",
    "smoothscroll-polyfill": "^0.4.4",
    "stripe": "^8.121.0",
    "ts-debounce": "^2.0.1",
    "webp-loader": "^0.6.0",
    "webpack-cli": "^3.3.12"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^9.5.3",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@zeit/next-source-maps": "0.0.3",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-unused-imports": "^0.1.3",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
